@model Productos

<h1>Agregar Productos</h1>

<div  class="container">
<form action="" method="post">  
    
  <div class="form-group">
    <label for="">Nombre</label>
    <input type="text" asp-for="nombre" class="form-control" id=""  placeholder="Ingrese Nombre">
    
  </div>
  <div class="form-group">
    <label for="">URL imagen:</label>
    <input type="text" asp-for="imagen" class="form-control" id=""  placeholder="Ingrese URL de imagen">
    
  </div>
  <div class="form-group">
    <label for="">Descripcion:</label>
    <input type="text" asp-for="descripcion" class="form-control" id=""  placeholder="Ingrese Descripcion">
    
  </div>
  <div class="form-group">
    <label for="">Precio:</label>
    <input type="number" asp-for="precio" class="form-control" id=""  placeholder="Ingrese Precio">
    
  </div>
  <div class="form-group">
    <label for="">Celular_Contacto</label>
    <input type="tel" asp-for="celular" class="form-control" id=""  placeholder="Ingrese NÃºmero de Celular">
    
  </div>
  <div class="form-group">
    <label for="">Lugar_Compra</label>
    <input type="text" asp-for="lugarcompra" class="form-control" id=""  placeholder="Ingrese Lugar de Compra">
    
  </div>
  <div class="form-group">
    <label for="exampleInputEmail1">Usuario</label>
    <input type="text" asp-for="usuario" class="form-control" id=""  placeholder="Ingrese usuario">
    
  </div>
  
  
  
  

  <button type="submit" class="btn btn-dark">Registrar</button>
</form>
</div>